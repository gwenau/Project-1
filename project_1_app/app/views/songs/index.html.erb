<h3><%= "List of Songs" %></h3>
  
<div class="home">
  <% @songs.each do |song| %>
  <%#= user_id. Undefined local variable at the moment. %>
  <%= song.song_name %>
  <br>
  <%= song.song_image %>
  <br>
  <%= song.song_file %>
  <br>
  <%= link_to 'Show', song %>
  <%= link_to 'Edit', edit_song_path(song) %>
  <%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure' } %>
  <br>
  <br>
  <% end %>
</div>