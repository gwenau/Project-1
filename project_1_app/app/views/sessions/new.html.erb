<h3>Login</h3>

<div class="home">
  <%= form_tag sessions_path do %>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
    <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <br>
    <%= submit_tag "Log in" %>
  <% end %>
  <br>
    <p>Please contact admin@admin.com if you need further assistance.</p>
    <br>
</div>
